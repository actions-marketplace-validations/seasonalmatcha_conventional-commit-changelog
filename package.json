{
  "name": "conventional-commit-changelog",
  "version": "0.0.1",
  "description": "Changelog generator from git tag using conventional commit",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "ncc build src/index.ts -o dist -m",
    "dev": "ts-node src/index.ts",
    "lint": "eslint .",
    "husky:install": "npx husky install",
    "postinstall": "npm run husky:install"
  },
  "keywords": [
    "Github",
    "Action",
    "Javascript",
    "Changelog",
    "Tag",
    "Tag Changelog"
  ],
  "author": "Seasonal Matcha <seasonalmatcha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
